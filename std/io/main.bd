import std::string

type file = int

#[link = "brad_file_open"]
extern fn open (path: str) (mode: str) -> file

#[link = "brad_file_write"]
extern fn write (file: file) (data: str) -> none

#[link = "brad_file_read"]
extern fn read (file: file) -> str

#[link = "brad_file_close"]
extern fn close (file: file) -> none

#[link = "brad_print"]
extern fn print (a: str) -> none

fn println (a: str) -> none {
  a |> string::append "\n" |> print
}
