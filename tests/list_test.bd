import std::list
import std::debug

fn main -> none {
  let l = list::empty
    |> list::prepend 1
    |> list::prepend 2
    |> list::prepend 3
    |> list::prepend "hello"
    |> list::filter is-odd
    |> list::map \x. x * 2 

  debug::print <| l
}

fn is-odd x => x % 2 == 1

fn mul x y => x * y
